{{#each this}}
     <div class="card">
          <blockquote class="blockquote">
               <p class="mb-0">{{text}}</p>
               <footer class="blockquote-footer font-italic">
                    <cite title="Commenter"><small>{{user.username}}, on {{format_date created_at}}</small></cite>
               </footer>
          </blockquote>
     </div>
{{/each}}